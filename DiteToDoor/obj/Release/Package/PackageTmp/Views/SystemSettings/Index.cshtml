                                                    @Html.EditorFor(model => model.CompanyInfo.Employee1, new { htmlAttributes = new { @class = "form-control" } })
                                                    @Html.ValidationMessageFor(model => model.CompanyInfo.Employee1, "", new { @class = "text-danger" })
                                                </div>

