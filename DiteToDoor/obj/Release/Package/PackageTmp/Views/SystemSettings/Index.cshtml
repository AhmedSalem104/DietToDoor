<h2>@DataMapping.Resources.Resource.GeneralSettings</h2>

<div class="row">
    @if (Model.CompanyInfo != null)
    {
        <input id="FiscalYearStartDatee" hidden value="@Model.CompanyInfo.FiscalYearStartDate.Date">
        <input id="FiscalYearEndDatee" hidden value="@Model.CompanyInfo.FiscalYearEndDate.Date">


    }
    @if (Model.InvoicesSettings != null)
    {
        <input id="InvoicesSettingIdd" hidden value="@Model.InvoicesSettings.InvoicesSettingId">


    }


    <div class="col-md-12">
        <div class="tab">
            <ul class="tabs">
                <li><a href="#">@DataMapping.Resources.Resource.GeneralSetting</a></li>
                <li><a href="#">إعدادات الحركات</a></li>
                @*<li><a href="#">>@DataMapping.Resources.Resource.Qutations</a></li>
                    <li><a href="#">@DataMapping.Resources.Resource.PurchaseOrders</a></li>
                    <li><a href="#">@DataMapping.Resources.Resource.Purchases</a></li>
                    <li><a href="#">@DataMapping.Resources.Resource.Documnets</a></li>*@

            </ul>

            <div class="tab_content" id="AllTabes">


                <div class="tabs_item">
                    @using (Html.BeginForm("AddOrEditCompanyInfo", "SystemSettings", FormMethod.Post, new { @class = "form-horizontal", @id = "ModalForm" }))

                    {
                        @*<div class="row">
                                <div class="col-12">
                                    <a href="#" class="btn btn-icon icon-left success_butn tab_button" onclick="showPopupCreateCountries()">
                                        @DataMapping.Resources.Resource.Add <i class="fa fa-plus" aria-hidden="true"></i>
                                    </a>
                                </div>
                            </div>*@
                        <div class="row">
                            <div class="col-12">
                                <button type="submit" class="btn btn-icon icon-left success_butn tab_button" id="submit">
