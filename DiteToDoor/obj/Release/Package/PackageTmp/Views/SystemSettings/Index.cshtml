                                                    @Html.ValidationMessageFor(model => model.CompanyInfo.CompanyManager, "", new { @class = "text-danger" })
                                                </div>

                                                <div class="form-group col-md-3">
                                                    @Html.HiddenFor(a => a.CompanyInfo.Id)
                                                    <label> رئيس الحركة والصيانة</label>

                                                    @Html.EditorFor(model => model.CompanyInfo.MaintananceManager, new { htmlAttributes = new { @class = "form-control" } })
                                                    @Html.ValidationMessageFor(model => model.CompanyInfo.MaintananceManager, "", new { @class = "text-danger" })
                                                </div>

                                                <div class="form-group col-md-3">
                                                    @Html.HiddenFor(a => a.CompanyInfo.Id)
                                                    <label> مراقب حركة</label>

                                                    @Html.EditorFor(model => model.CompanyInfo.MovementManager, new { htmlAttributes = new { @class = "form-control" } })
                                                    @Html.ValidationMessageFor(model => model.CompanyInfo.MovementManager, "", new { @class = "text-danger" })
                                                </div>

                                                <div class="form-group col-md-3">
